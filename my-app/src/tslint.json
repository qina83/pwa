{
    "extends": "../tslint.json",
    "rules": {
        "curly": [true, "as-needed"],
        "one-line": [false],
        "directive-selector": [
            true,
            "attribute",
            "app",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "app",
            "kebab-case"
        ]
    }
}
